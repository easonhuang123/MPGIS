联站', '吉祥站', '龙城广场站', '南联站', '双龙站']\n            ],\n            show: false,\n            begin: '',\n            end: '',\n            hash: ''\n        }\n        this.showlist = this.showlist.bind(this)\n        this.choosenode = this.choosenode.bind(this)\n    }\n\n    showlist (key) {\n        this.setState({\n            show: true,\n            hash: key\n        })\n    }\n\n    choosenode (node) {\n        if (node) {\n           if (this.state.hash === 'begin') {\n                this.setState({\n                    begin: node,\n                    show: false\n                })\n            } else {\n                this.setState({\n                    end: node,\n                    show: false\n                })\n            } \n        } else {\n            this.setState({\n                show: false\n            })\n        }\n    }\n\n    render () {\n        return (\n            <div className='path'>\n                <StationSelect showlist={this.showlist} begin={this.state.begin} end={this.state.end}/>\n                <Recommand />\n\n                {!!this.state.show &&\n                    <StationList nodelist={this.state.nodelist}\n                                 choosenode={this.choosenode}/>\n                }\n            </div>\n        )\n    }\n}\n\nexport default Path"},"lineCount":null}},"hash":"dffeb20185d7fca7403a22a6da41b735","cacheData":{"env":{}}}